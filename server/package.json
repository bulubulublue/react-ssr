{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "Server side rendering project",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:build-client": "npx webpack --config webpack.client.js --watch",
    "dev:build-server": "npx webpack --config webpack.server.js --watch",
    "dev:server": "nodemon --watch build --exec \"node build/bundle.js\""
  },
